							"    pm.expect(response.response).to.have.property('UpdateDate');",
							"    pm.expect(response).to.have.property('status');",
							"});",
							"",
							"pm.test(\"Response fields have correct types\", function () {",
							"    let response = pm.response.json();",
							"    pm.expect(response.response.Id).to.be.a('number');",
							"    pm.expect(response.response.UserId).to.be.a('number');",
							"    pm.expect(response.response.title).to.be.a('string');",
							"    pm.expect(response.response.Description).to.be.a('string');",
							"    pm.expect(response.response.HasMultimedia).to.be.a('boolean');",
							"    pm.expect(response.response.Public).to.be.a('boolean');",
							"    pm.expect(response.response.Multimedia).to.be.a('string');",
							"});",
							"",
							"pm.test(\"Status is SUCCESS\", function () {",
							"    let response = pm.response.json();",
							"    pm.expect(response.status).to.eql(\"SUCCESS\");",
