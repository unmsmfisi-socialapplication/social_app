    "Succesful Import of profile": {
        "description": "Import a user profile with valid data",
        "request": {
            "method": "POST",
            "url": "https://31f26148-53be-474d-9a51-fa125946327a.mock.pstmn.io/importar-perfil",
            "headers": {
                "Content-Type": "application/json"
            },
            "body": {
                "type": {
                    "type": "string",
                    "value": "ImportProfile",
                    "description": "Type of the import request."
                },
                "actor": {
                    "type": "string",
                    "value": "SofiaRV",
                    "description": "actor's profile."
                },
                "name": {
                    "type": "string",
                    "value": "Perfil de Sofia",
                    "description": "Name of the profile to import."
                },
                "object": {
                    "type": "object",
                    "value": {
                        "id": {
                            "type": "string",
                            "value": "1234",
                            "description": "Unique identifier of the profile."
                        },
                        "type": {
                            "type": "string",
                            "value": "Person",
                            "description": "Type of the object, indicating it's a person."
                        },
                        "name": {
                            "type": "string",
                            "value": "Pepe Rodriguez",
                            "description": "Name of the person."
                        },
                        "username": {
                            "type": "string",
                            "value": "PepeR",
                            "description": "Username of the person."
                        },
                        "hasMultimedia": {
                            "type": "boolean",
                            "value": true,
                            "description": "Indicates if the profile has multimedia content."
                        },
                        "public": {
                            "type": "boolean",
                            "value": true,
                            "description": "Indicates if the profile is public."
                        },
                        "summary": {
                            "type": "string",
                            "value": "Amante de la naturaleza y entusiasta de la tecnología.",
                            "description": "Brief summary about the person."
                        },
                        "profileImage": {
                            "type": "string",
                            "value": "https://appsocial.com/sofia/profileImage.jpg",
                            "description": "URL of the person's profile image."
                        },
                        "endpoints": {
                            "type": "object",
                            "value": {
                                "sharedInbox": {
                                    "type": "string",
                                    "value": "https://appsocial.com/inbox",
                                    "description": "URL of the shared inbox endpoint."
                                }
                            },
                            "description": "Endpoints associated with the profile."
                        },
                        "importedFrom": {
                            "type": "string",
                            "value": "https://mastodon.ejemplo.com/@sofiR",
                            "description": "Original source from where the profile is imported."
                        }
                    },
                    "description": "Detailed information of the profile."
                },
                "to": {
                    "type": "array",
                    "value": ["https://appsocial.com/system/import"],
                    "description": "Destination array for the import."
                },
                "cc": {
                    "type": "array",
                    "value": [],
                    "description": "Carbon copy array."
                }
            }
        },
        "response": {
            "status": 200,
            "body": {
                "status": "OK",
                "message": "Profile imported successfully"
            }
        }
    },
