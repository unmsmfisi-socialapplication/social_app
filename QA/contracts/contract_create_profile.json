{
    "Succesful Profile Creation": {
        "description": "Create a new user profile with valid details",
        "request": {
            "method": "POST",
            "url": "https://51790c60-29ec-429c-9ce1-5da259f03d0d.mock.pstmn.io/crear-perfil",
            "headers": {
                "Content-Type": "application/json"
            },
            "body": {
                "type": {
                    "type": "string",
                    "value": "Person",
                    "description": "Type of the profile."
                },
                "actor": {
                    "type": "string",
                    "value": "JhonDP_1",
                    "description": "Actor of the profile."
                },
                "name": {
                    "type": "string",
                    "value": "John Doe",
                    "description": "Name of the user."
                },
                "username": {
                    "type": "string",
                    "value": "johndoe",
                    "description": "Username of the user."
                },
                "hasMultimedia": {
                    "type": "boolean",
                    "value": true,
                    "description": "Indicates if the profile has multimedia."
                },
                "public": {
                    "type": "boolean",
                    "value": true,
                    "description": "Indicates if the profile is public."
                },
                "summary": {
                    "type": "string",
                    "value": "Experienced web developer with a passion for responsive design and user experience.",
                    "description": "Summary or biography of the user."
                },
                "profileImage": {
                    "type": "string",
                    "value": "https://example.com/images/johndoe.jpg",
                    "description": "URL of the user's profile image."
                },
                "endpoints": {
                    "type": "object",
                    "value": {
                        "sharedInbox": "https://example.com/inbox/johndoe"
                    },
                    "description": "Endpoints associated with the profile."
                }
            }
        },
        "response": {
            "status": 200,
            "body": {
                "status": "OK",
                "message": "Profile created successfully"
            }
        }
    },
