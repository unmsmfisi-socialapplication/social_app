{
    "Successful Login": {
        "description": "Successful Login",
        "request": {
            "method": "POST",
            "url": "https://31f26148-53be-474d-9a51-fa125946327a.mock.pstmn.io/login",
            "headers": {
                "Content-Type": "application/json"
            },
            "body": {
                "user_name": {
                    "type": "string",
                    "minLength": 6,
                    "maxLength": 56,
                    "pattern": "^[a-zA-Z0-9]+$"
                },
                "password": {
                    "type": "string",
                    "minLength": 8,
                    "maxLength": 46,
                    "pattern": "^(?=.*[a-zA-Z])(?=.*[0-9])(?=.*[@#$%^&+=]).*$"
                }
            }
        },
        "response": {
            "status": 200
        },
        "required": ["user_name", "password"]
    },
